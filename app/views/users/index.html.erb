<div class="user">
    <h1>All users</h1>

    <table>
        <tr>
            <th>Username</th>
            <th>Karma</th>
        </tr>
        <% @users.each do |user| %>
        <tr>
            <td><%= link_to user.username, user_path(user) %></td>
            <td>0</td>
        </tr>
        <% end %>
    </table>
</div>